{
	"version": 2,
	"public": false,
	"builds": [{ "src": "api/main.py", "use": "@vercel/python" }],
	"routes": [
		{ "src": "/", "dest": "api/main.py" },
		{ "src": "/docs", "dest": "api/main.py" },
		{ "src": "/openapi.json", "dest": "api/main.py" }
	],
	"env": {
    "APP_MODULE": "main:app"
  },
	"buildCommand": "chmod +x install.sh && ./install.sh"
}