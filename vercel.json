{
	"version": 2,
	"public": false,
	"builds": [{ "src": "api/main.py", "use": "@vercel/python" }],
	"routes": [
		{ "src": "/", "dest": "api/main.py" },
		{ "src": "/docs", "dest": "api/main.py" },
		{ "src": "/openapi.json", "dest": "api/main.py" }
	],
	"env": {
    "APP_MODULE": "main:app"
  },
	"buildCommand": "sudo apt install libpango-1.0-0 libharfbuzz0b libpangoft2-1.0-0 libffi-dev libjpeg-dev libopenjp2-7-dev"
}